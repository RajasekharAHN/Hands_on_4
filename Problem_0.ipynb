      "source": [
        "def fib(n):\n",
        "    if n == 0:\n",
        "        return 0\n",
        "    if n == 1:\n",
        "        return 1\n",
        "    return fib(n-1) + fib(n-2)\n",
        "\n",
        "def debug_fib(n):\n",
        "    result = fib(n)\n",
        "    print(f\"fib({n}) = {result}\")\n",
        "    return result\n",
        "\n",
        "n = int(input(\"Enter any value to calculate Fibonacci sequence(n): \"))\n",
        "debug_fib(n)"
      ]
